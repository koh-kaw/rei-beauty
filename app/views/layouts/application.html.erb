  <body>
    <%# unless current_page?(new_user_session_path) %>
    <%# unless request.path.include?("sign_in") || request.path.include?("sign_up") %>
    <%= render 'shared/header' %>
    <%# end %>
    <% if content_for?(:body_attributes) %>
      <%= yield(:body_attributes) %>
    <% end %>

    <%= yield %>

    <%= render 'shared/footer'%>
