    <%= stylesheet_pack_tag 'custom', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'calendar', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= stylesheet_pack_tag 'bootstrap/css/bootstrap', media: 'all', 'data-turbolinks-track': 'reload' %>
